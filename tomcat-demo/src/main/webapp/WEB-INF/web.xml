<!DOCTYPE web-app PUBLIC
        "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
    <display-name>Archetype Created Web Application</display-name>
    <servlet>
        <servlet-name>demo-start</servlet-name>
        <servlet-class>com.example.DemoController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>demo-start</servlet-name>
        <url-pattern>/demo-start</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>demo-start2</servlet-name>
        <servlet-class>com.example.DemoController2</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>demo-start2</servlet-name>
        <url-pattern>/demo-start2</url-pattern>
    </servlet-mapping>
</web-app>
